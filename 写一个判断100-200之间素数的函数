#include <math.h>
#include <stdio.h>
int prime(int i) {
	int j = 0;
	for (j = 2; j <= sqrt(i); j++) {
		if (i % j == 0) {
			return 0;
		}
	}
	return 1;
}
int main() {
	int count = 0;
	int i = 0;
	for (i = 101; i < 200; i += 2) {
		if (prime(i)) {
			printf("%d ", i);
			count++;
		}
	}
	printf("\n%d", count);
	return 0;
}
